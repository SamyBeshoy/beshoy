block addGallery
    .galleryJS
    script.
        let gallery = document.querySelector(".galleryJS");
        for (let i = 18; i > 0; i--) {
            let img = document.createElement("img");
            img.src = `imgs/gallery-${i}.jpg`;
            gallery.append(img);
            img.className = `img-${i} imgs`;
        }
